<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#ececec">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:background="@android:color/transparent"
        android:contentDescription="Back button"
        android:src="@drawable/arrow_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/imageButton">

        <LinearLayout
            android:id="@+id/LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="10dp">
            <LinearLayout
                android:id="@+id/LinearLayout_VFR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="10dp">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ConstraintLayout_VFR"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/TextView_VFR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Friend Requests"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintend_toStartOf="parent"
                        tools:ignore="HardcodedText" />
                    <ImageView
                        android:id="@+id/ImageView_VFR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/expand_less"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:contentDescription="expand section"
                        tools:ignore="HardcodedText" />
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/FriendRequests_PBG"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:importantForAccessibility="no"
                        app:srcCompat="@drawable/circle_bg"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                    <ImageView
                        android:id="@+id/FriendRequests_PFP"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:importantForAccessibility="no"
                        app:srcCompat="@drawable/default_pfp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                    <TextView
                        android:id="@+id/FriendRequests_username"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Username"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        app:layout_constraintStart_toEndOf="@+id/FriendRequests_PBG"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="5dp"
                        tools:ignore="HardcodedText" />
                    <TextView
                        android:id="@+id/FriendRequests_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="name"
                        android:textColor="#5C5A5A"
                        android:textSize="14sp"
                        app:layout_constraintEnd_toEndOf="@+id/FriendRequests_username"
                        app:layout_constraintStart_toStartOf="@+id/FriendRequests_username"
                        app:layout_constraintTop_toBottomOf="@+id/FriendRequests_username"
                        tools:ignore="HardcodedText" />
                    <ImageView
                        android:id="@+id/FriendRequests_Deny"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:src="@drawable/close"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:contentDescription="Deny Friend Request"
                        tools:ignore="HardcodedText"/>
                    <ImageView
                        android:id="@+id/FriendRequests_Accept"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:src="@drawable/check"
                        app:layout_constraintEnd_toStartOf="@+id/FriendRequests_Deny"
                        app:layout_constraintTop_toTopOf="@+id/FriendRequests_Deny"
                        android:contentDescription="Accept Friend Request"
                        tools:ignore="HardcodedText"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_VF"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="10dp">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ConstraintLayout_VF"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp">
                    <TextView
                        android:id="@+id/TextView_VF"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Friends"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintend_toStartOf="parent"
                        tools:ignore="HardcodedText" />
                    <ImageView
                        android:id="@+id/ImageView_VF"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/expand_less"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:contentDescription="expand section"
                        tools:ignore="HardcodedText"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/Friends_PBG"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:importantForAccessibility="no"
                        app:srcCompat="@drawable/circle_bg"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                    <ImageView
                        android:id="@+id/Friends_PFP"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:importantForAccessibility="no"
                        app:srcCompat="@drawable/default_pfp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                    <TextView
                        android:id="@+id/Friends_username"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Username"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        app:layout_constraintStart_toEndOf="@+id/Friends_PBG"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="5dp"
                        tools:ignore="HardcodedText" />
                    <TextView
                        android:id="@+id/Friends_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="name"
                        android:textColor="#5C5A5A"
                        android:textSize="14sp"
                        app:layout_constraintEnd_toEndOf="@+id/Friends_username"
                        app:layout_constraintStart_toStartOf="@+id/Friends_username"
                        app:layout_constraintTop_toBottomOf="@+id/Friends_username"
                        tools:ignore="HardcodedText" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_VG"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="10dp">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ConstraintLayout_G"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/TextView_G"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Groups"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintend_toStartOf="parent"
                        tools:ignore="HardcodedText" />
                    <ImageView
                        android:id="@+id/ImageView_G"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/expand_less"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:contentDescription="expand section"
                        tools:ignore="HardcodedText"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/Groups_PBG"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:importantForAccessibility="no"
                        app:srcCompat="@drawable/circle_bg"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                    <ImageView
                        android:id="@+id/Groups_PFP"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:importantForAccessibility="no"
                        app:srcCompat="@drawable/default_pfp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                    <TextView
                        android:id="@+id/Groups_Name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Group Name"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        app:layout_constraintStart_toEndOf="@+id/Groups_PBG"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="5dp"
                        tools:ignore="HardcodedText" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>

        </LinearLayout>



    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>